---

interface Props extends astroHTML.JSX.ButtonHTMLAttributes{
    title: string;
}

const { title } = Astro.props;
---
<button
type="submit"
class="text-white absolute end-2.5 bottom-2.5 bg-primary hover:bg-primaryHover focus:ring-4 focus:outline-none focus:ring-secondary font-medium rounded-lg text-sm px-4 py-2"
>{title}</button>
